{
    "name": "chenweibo/laravelcmsfile",
    "description": "The laravelcms extension package is used for file operations",
    "license": "MIT",
    "authors": [
        {
            "name": "chenweibo",
            "email": "563960993@qq.com",
            "homepage": ""
        }
    ],
    "homepage": "https://github.com/chenweibo/lavarel-cms-filesystem",
    "keywords": ["Laravel", "LaravelCmsFile"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "league/flysystem": "^1.0.47"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "chenweibo\\LaravelCmsFile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "chenweibo\\LaravelCmsFile\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "chenweibo\\LaravelCmsFile\\LaravelCmsFileServiceProvider"
            ],
            "aliases": {
                "LaravelCmsFile": "chenweibo\\LaravelCmsFile\\Facades\\LaravelCmsFile"
            }
        }
    }
}
